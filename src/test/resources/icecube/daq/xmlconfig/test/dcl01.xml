<?xml version="1.0" encoding="UTF-8"?>
<domconfiglist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../domconfiglist.xsd">
    <trigger name="standard-spe" mode="spe">
        <spe>560</spe>
        <mpe>555</mpe>
    </trigger>

    <format name="inice-eng-short" compression="off">
        <engineeringformat>
            <atwd ch="0" samples="128"/>
            <atwd ch="1" samples="128"/>
            <atwd ch="2" samples="128"/>
            <atwd ch="3" samples="0"/>
            <fadc samples="150"/>
        </engineeringformat>
    </format>

    <localcoincidence name="inice-slc-x">
        <type>SLC</type>
        <mode>up or down</mode>
        <span>2</span>
        <pretrigger>1200</pretrigger>
        <posttrigger>1250</posttrigger>
        <cablelen>
            <up>500 1250 2000 2750</up>
            <down>575 1225 1975 2925</down>
        </cablelen>
    </localcoincidence>
    
    <domconfig name="inice-a-dom">
        <trigger base="standard-spe"/>
        <format base="inice-eng-short"/>
        <localcoincidence base="inice-slc-x"/>
        <mux>off</mux>
        <pulser mode="beacon"><rate>5</rate></pulser>
        <hv>0</hv>
        <supernova>
            <mode>spe</mode>
            <deadtime>51200</deadtime>
        </supernova>
    </domconfig>
    
    <domconfig mbid="5b772030332a" base="inice-a-dom">
        <hv>2760</hv>
        <supernova>
            <mode>mpe</mode>
            <deadtime>95481</deadtime>
        </supernova>
    </domconfig>
    
    <domconfig mbid="007ca4ab2311" base="inice-a-dom">
        <hv>2625</hv>
        <supernova>
            <mode>spe</mode>
            <deadtime>51200</deadtime>
        </supernova>
        <scalerdeadtime>6400</scalerdeadtime>
    </domconfig>
    
</domconfiglist>
